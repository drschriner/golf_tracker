  <h1>Create a New Course</h1>

    <div id="content2">

      <form action="/course/create" method="POST">
        <input style="margin-left: 10%; margin-top: 5%" type="text" placeholder="Course Name" name="name"> <br/>
        <input style="margin-left: 10%" type="text" placeholder="City" name="city"> <br/>
        <input style="margin-left: 10%" type="text" placeholder="State" name="state"> <br/>
        <input style="margin-left: 10%" type="text" placeholder="Zip Code" name="zip"> <br/>
        <input style="margin-left: 10%" type="text" placeholder="Course Par" name="par"> <br/>
        <input style="margin-left: 10%" type="text" placeholder="Course Yardage" name="yardage"> <br/>
        <input style="margin-left: 10%" type="text" placeholder="Description" name="description"> <br/>
        <br>
        <input style="margin-left: 10%" type="submit" value="Create Course" />
      </form>

    </div>
